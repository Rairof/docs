{
	"$schema": "https://mintlify.com/schema.json",
	"name": "Unbound",
	"logo": {
		"dark": "/logo/dark.svg",
		"light": "/logo/light.svg"
	},
	"favicon": "/logo/dark.svg",
	"colors": {
		"primary": "#c74050",
		"light": "#c74050",
		"dark": "#c74050",
		"background": {
			"dark": "#0c0c0c",
			"light": "#fcfcfc"
		},
		"anchors": {
			"from": "#c74050",
			"to": "#9e333f"
		}
	},
	"modeToggle": {
		"default": "dark",
		"isHidden": false
	},
	"backgroundImage": "/images/background.png",
	"anchors": [
		{
			"name": "Discord Server",
			"icon": "discord",
			"url": "https://discord.gg/unbound"
		},
		{
			"name": "GitHub",
			"icon": "github",
			"url": "https://github.com/unbound-mod"
		}
	],
	"topAnchor": {
		"name": "Home",
		"icon": "building"
	},
	"feedback": {
		"suggestEdit": true,
		"raiseIssue": true
	},
	"search": {
		"prompt": "Search documentation..."
	},
	"navigation": [
		{
			"group": "Documentation",
			"pages": [
				"documentation/overview"
			]
		},
		{
			"group": "Loader",
			"icon": "arrow-right-to-bracket",
			"pages": [
				"loader/introduction",
				"loader/configuration",
				"loader/local-build",
				"loader/ios",
				"loader/android"
			]
		},
		{
			"group": "API",
			"icon": "folder-tree",
			"pages": [
				"api/metro",
				"api/patcher",
				"api/storage",
				"api/assets",
				"api/toasts",
				"api/dialogs",
				"api/commands",
				"api/native"
			]
		}
	],
	"footerSocials": {
		"twitter": "https://twitter.com/UnboundClient",
		"github": "https://github.com/unbound-mod",
		"discord": "https://discord.gg/unbound"
	}
}